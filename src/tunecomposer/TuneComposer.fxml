<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Line?>


<ScrollPane prefHeight="918.0" prefWidth="1721.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tunecomposer.TuneComposer">
   <content>
      <BorderPane id="root-pane" prefHeight="1369.0" prefWidth="2022.0" stylesheets="@tunecomposer.css">
         <top>
            <MenuBar>
              <menus>
                  <Menu mnemonicParsing="false" text="Action">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#handlePlayScaleButtonAction" text="Play" />
                        <MenuItem mnemonicParsing="false" onAction="#handleStopPlayingButtonAction" text="Stop" />
                    </items>
                  </Menu>
                <Menu text="File">
                  <items>
                    <MenuItem onAction="#handleExitMenuItemAction" text="Exit" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <right>
            <HBox id="button-box" BorderPane.alignment="CENTER">
               <children>
                  <Line endX="100.0" startX="-100.0" stroke="#00000059" />
               </children>
            </HBox>
         </right>
      </BorderPane>
   </content>
</ScrollPane>
